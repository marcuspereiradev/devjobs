
<%= render './header' %>
<section class="container row">
  <div class="col s4">
    <ul class="collection">
      <% @users.each do |user| %>
        <li class="collection-item avatar vacancy-items">
          <%= image_tag 'vacancy.jpg', class: 'circle' %>
          <span class="title title-vancancy"><%= user.title %></span>
          <p><%= user.name %></p>
          <p><%= user.location %></p>
          <p><%= user.website %></p>
          <p><%= user.email %></p>
          <span class="secondary-content"><i class="material-icons">grade</i></span>
        </li>
      <% end %>
    </ul>
  </div>
  <div class="col s8">
    <div class="section">
    
    <div>
  </div>
</section>